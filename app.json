{
  "expo": {
    "name": "Scratby Premier",
    "slug": "scratbypremier",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#4c0743"
      },
      "bundleIdentifier": "com.gerrytucker.scratbypremier"
    },
    "android": {
      "splash": {
        "hdpi": "./assets/splash-hdpi.png",
        "ldpi": "./assets/splash-ldpi.png",
        "mdpi": "./assets/splash-mdpi.png",
        "xhdpi": "./assets/splash-xhdpi.png",
        "xxhdpi": "./assets/splash-xxhdpi.png",
        "xxxhdpi": "./assets/splash-xxxhdpi.png",
        "resizeMode": "cover"
      },
      "package": "com.gerrytucker.scratbypremier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
